{
  "name": "angular-cli-electron-sqlite-bootstrap",
  "productName": "Angular-cli-Electron-SQLite-Bootstrap",
  "description": "An Electron application using Angular-cli, Sqlite and Bootstrap",
  "author": {
    "name": "pamtbaau"
  },
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "build:once": "ng build --base-href ./",
    "build:watch": "ng build --base-href ./ --watch",
    "test": "ng test",
    "test:codeCoverage": "ng test --codeCoverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.pamtbaau.angular-cli-electron-sqlite-bootstrap",
    "productName": "Angular-cli-Electron-SQLite-Bootstrap",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false
    },
    "files": [
      "!**/.vscode",
      "!**/build",
      "!**/config",
      "!**/assets/data",
      "!**/src"
    ],
    "extraResources": [
      {
        "from": "src/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "bootstrap": "^4.4.1",
    "electron-reload": "^1.5.0",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "karma-electron": "^6.3.0",
    "node-pre-gyp": "^0.15.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "sqlite3": "^4.2.0",
    "tslib": "^1.11.1",
    "xlsx": "^0.16.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.29",
    "@types/sqlite3": "^3.1.6",
    "codelyzer": "^5.2.2",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
